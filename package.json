{
  "name": "commit-ai",
  "version": "1.1.0",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "commit-ai": "./src/index.ts",
    "git-commit-ai": "./src/index.ts",
    "commit-ai-build": "tsc",
    "git-ai": "./bin/commit-ai.exe"
  },
  "scripts": {
    "start": "bun run index.ts",
    "build": "tsc",
    "build-exe": "bun build ./src/index.ts --compile --outfile ./bin/commit-ai.exe",
    "link-cmd": "bun link && bun link commit-ai",
    "full-build": "bun run build && bun run build-exe && npm link --force"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "dotenv": "^17.2.3",
    "groq-sdk": "^0.37.0",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0"
  },
  "module": "index.ts",
  "private": true,
  "peerDependencies": {
    "typescript": "^5"
  }
}
